<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
    
    <bean id="customScriptTaggingService" parent="baseJavaScriptExtension" class="org.alfresco.module.iptech.ScriptTagManagementService">
        <property name="extensionName">
            <value>tagManagementService</value>
        </property>
        <!-- Creates ScriptNodes which require the ServiceRegistry -->
        <property name="serviceRegistry" ref="ServiceRegistry"/>
    </bean>
    
</beans>
